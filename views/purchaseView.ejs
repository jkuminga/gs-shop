<table class="table table-bordered border-primary table-hover" style="text-align: center;">
    <thead class="table-primary "><th>고객ID</th><th>고객명</th><th>상품 ID</th><th>상품명</th><th>구매일</th><th>가격</th><th>포인트</th><th>구매량</th><th>총가격</th><th>지불여부</th><th>취소여부</th><th>환불여부</th><th colspan="2"></th></thead> 
    <% var i = 0;
    while(i < purchase.length) { %>
    <tr><td><%= purchase[i].loginid %></td>
        <td><%= purchase[i].name %></td>
        <td><%= purchase[i].mer_id %></td>
        <td><%= purchase[i].mer_name %></td>
        <td><%= purchase[i].date %></td>
        <td><%= purchase[i].price %> 원</td>
        <td><%= purchase[i].point %></td>
        <td><%= purchase[i].qty %></td>
        <td><%= purchase[i].total %> 원</td>
        <td><%= purchase[i].payYN %></td>
        <td><%= purchase[i].cancel %></td>
        <td><%= purchase[i].refund %></td>
        <td><a href="/purchaseupdate/<%= purchase[i].purchase_id %>">수정</a></td>
        <td><a href="/purchasedelete/<%= purchase[i].purchase_id %>" onclick="if(confirm('정말로 삭제하시겠습니까?')==false) return false">삭제</a></td>
    </tr>
    <% i++; } %>
</table>

<% if (purchase.length == 0) { %>
<p>구매 내역이 없습니다. </p>
<% } %>
